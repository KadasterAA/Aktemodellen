	<xs:complexType name="IMKAD_AangebodenStuk">
		<xs:annotation>
			<xs:documentation>Een ter inschrijving aangeboden stuk is een stuk dat ter inschrijving in de Openbare registers is aangeboden. Deze stukken worden aangeboden door het notariaat, deurwaarders, (lagere) overheden. etc.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ondertekenaar" type="tia:IMKAD_Ondertekenaar" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De NAW gegevens van degene die het stuk als / namens de aanbieder heeft ondertekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_Verklaarder" type="tia:Verklaarder" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gegevens van de persoon die de equivalentieverklaring heeft ondertekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_DatumOndertekening" type="alg:Datum" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Datum waarop een ter inschrijving aangeboden stuk is ondertekend door de opsteller van het stuk.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_TijdOndertekening" type="xs:time" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tijdstip waarop een ter inschrijving aangeboden stuk is ondertekend door de opsteller van het stuk.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_OmschrijvingKenmerk" type="alg:alg-AN40" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De identificatie van het aangeboden stuk bij de aanbieder. In het geval van een hypotheek betreft dit het offertenummer (van de bank).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_OmschrijvingAnnexen" type="alg:alg-AN40" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Verwijzing naar de bijlagen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_TekstTweedeDeel" type="xs:anyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Vrije tekst, die na de registratieclausule in de akte ogenomen kan worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="tia:Gevolmachtigde" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="tia:Partij" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="tia_TekstKeuze" type="tia:TekstKeuze" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tekst die door een aanbieder gekozen is voor een variabele tekst.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_AantalOnderhandseAktenVolmacht" type="alg:alg-N3" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het aantal akten van volmacht die aan de akte worden gehecht. Wordt vermeld in de akte bij doorhaling hypotheken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_PlaatsOndertekening" type="tia:BAG_Woonplaats" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Plaats waar de akte ondertekend is. Dit kan een andere plaats zijn dan de locatie waar het notariskantoor gevestigd is. Wordt vermeld in de akte bij doorhaling hypotheken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_StukVariant" type="alg:Waardenlijst" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Wanneer binnen een stuk meerdere varianten kunnen voorkomen wordt hier de gekozen variant opgenomen. Bijvoorbeeld:
de gekozen koop-levering variant binnen de akte van levering.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_OmschrijvingZaak" type="alg:alg-AN40" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De identificatie van het dossier waartoe een stuk behoort bij de aanbieder. (Veelal gebruik bij hypotheken, omdat het kenmerk dan een identificatie van de bank bevat).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_DepotnummerTekening" type="alg:alg-AN14" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het depotnummer van de tekening, die vooraf in depot is gegeven en waar in het stuk naar wordt verwezen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_WoonplaatskeuzePlaatsnaam" type="alg:alg-AN250" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Plaatsnaam van het hoofdkantoor van de bank (Nationale Nederlanden)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="tia:IMKAD_Stukdeel" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="tia:Hoedanigheid" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="heeftVerklaarder" type="tia:Notaris" minOccurs="0" maxOccurs="1"/>
			<xs:element name="heeftOndertekenaar" type="tia:Notaris" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="BAG_NummerAanduiding" type="tia:BAG_NummerAanduiding"/>
	<xs:complexType name="BAG_NummerAanduiding">
		<xs:annotation>
			<xs:documentation>Een NUMMERAANDUIDING is een door de gemeenteraad als
zodanig toegekende aanduiding van een adresseerbaar object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="huisnummer" type="alg:alg-N5" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Een door of namens het gemeentebestuur ten aanzien
van een adresseerbaar object toegekende nummering.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="huisletter" type="alg:Huisletter" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Een door of namens het gemeentebestuur ten aanzien
van een adresseerbaar object toegekende toevoeging
aan een huisnummer in de vorm van een alfanumeriek
teken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="huisnummertoevoeging" type="alg:alg-AN10" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Een door of namens het gemeentebestuur ten aanzien
van een adresseerbaar object toegekende nadere
toevoeging aan een huisnummer of een combinatie van
huisnummer en huisletter.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postcode" type="alg:Postcode" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De door TNT Post vastgestelde code behorende bij een
bepaalde combinatie van een straatnaam en een
huisnummer.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="IMKAD_PostlocatiePersoon" type="tia:IMKAD_PostlocatiePersoon"/>
	<xs:complexType name="IMKAD_PostlocatiePersoon">
		<xs:annotation>
			<xs:documentation>Een Postlocatie (waar een persoon post ontvangt) kan een binnenlandse locatie, een buitenlandse locatie of een postbusadres zijn. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tia_label" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organisatie-onderdeel dat de correspondentie van de betrokken (rechts)persoon verzorgt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_afdeling" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Afdeling binnen het organisatie-onderdeel dat de correspondentie van de betrokken (rechts)persoon verzorgt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adres" type="tia:Imkad_AdreskeuzePl" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Keuze tussen PostbusAdres, BinnenlandsAdres en BuitenlandsAdres.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TeboekgesteldeZaak" type="tia:TeboekgesteldeZaak" abstract="true" substitutionGroup="tia:KadastraalObject"/>
	<xs:complexType name="TeboekgesteldeZaak" abstract="true">
		<xs:annotation>
			<xs:documentation>Een teboekgestelde zaak is een Kadastraal object dat bestaat uit een Luchtvaartuig of een Schip.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tia:KadastraalObject">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="IMKAD_Appartementsrecht" type="tia:IMKAD_Appartementsrecht" substitutionGroup="tia:IMKAD_OnroerendeZaak"/>
	<xs:complexType name="IMKAD_Appartementsrecht">
		<xs:annotation>
			<xs:documentation>Een appartementsrecht is onroerende zaak dat kadastraal is aangeduid, bestaande uit een aandeel in recht(en) op de onroerende zaak/zaken die in de splitsing is/zijn betrokken en dat de bevoegdheid bevat tot het uitsluitend gebruik van bepaalde gedeelten van een onroerende zaak, die blijkens hun inrichting bestemd zijn of worden om als afzonderlijk geheel te worden gebruikt. Het aandeel kan omvatten de bevoegdheid tot het uitsluitend gebruik van bepaalde gedeelten van een gebouw en van de bij het gebouw behorende grond (definitie is ontleend aan art 2, Kadasterbesluit).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tia:IMKAD_OnroerendeZaak">
				<xs:sequence>
					<xs:element ref="tia:Aandeel" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Aandeel in de omschreven gemeenschap.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="IMKAD_KadastraleAanduiding" type="tia:IMKAD_KadastraleAanduiding"/>
	<xs:complexType name="IMKAD_KadastraleAanduiding">
		<xs:annotation>
			<xs:documentation>Kadastrale aanduiding is de unieke aanduiding van een onroerende zaak, die door het kadaster wordt vastgesteld.

Voor netwerken wordt deze als volgt gevuld:
Kadastrale gmeente = NWKxx waarbij xx het nummer van de dabase van het Kadaster is  (dus 01-16)
Sectie = T, E, W, G, D (diversen) of O. (zie ook  tabel aard LeidingNetwerk.

Percelen worden kadastraal aangeduid door vermelding van achtereenvolgens de kadastrale gemeente en sectie, waarin de percelen en  gedeelten van percelen zijn gelegen waarvan het grondgebied tot die zaak behoort, alsmede de nummers van die percelen. Voor een onroerende zaak die zich krachtens een opstalrecht op, in of boven de grond van een ander bevindt, geldt dezelfde kadastrale aanduiding als van de onroerende zaak die met dat opstalrecht is bezwaard. Dit is van overeenkomstige toepassing op een onroerende zaak die zich op, in of boven de grond van een ander bevindt krachtens een recht als bedoeld in het voor 1 januari 1992 geldende artikel 5, derde lid, onder b, laatste zinsnede, van de Belemmeringenwet Privaatrecht. 

Appartementsrechten worden kadastraal aangeduid door de vermelding van achtereenvolgens de kadastrale gemeente en sectie, waarin de in de splitsing betrokken percelen zijn gelegen, de complexaanduiding en de appartementsindex. De complexaanduiding bestaat uit het voor de in de splitsing betrokken percelen vastgestelde complexnummer, gevolgd door de hoofdletter A. Onze Minister stelt regelen vast omtrent de vaststelling van het complexnummer. Ingeval de in de splitsing in appartementsrechten betrokken percelen in verschillende burgerlijke gemeenten zijn gelegen is het tweede lid van overeenkomstige toepassing met dien verstande, dat in dat geval de bewaarder bepaalt van welke van de betrokken kadastrale gemeenten de naam wordt vermeld in de vast te stellen kadastrale aanduiding van de appartementsrechten waarin die percelen worden gesplitst. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="gemeente" type="alg:Waardenlijst" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De kadastrale gemeente, deel van de kadastrale aanduiding van de onroerende zaak. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sectie" type="alg:alg-AN2V" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De sectie die de sectie binnen de kadastrale gemeente uniek identificeert.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="perceelnummer" type="alg:alg-N5" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het perceelnummer dat een geheel perceel of een complex uniek identificeert binnen de sectie.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="appartementsindex" type="alg:alg-N4" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nummer dat het kadastraal object uniek identificeert als een appartementsrecht binnen het complex.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="GegevensGevolmachtigde" type="tia:GegevensGevolmachtigde"/>
	<xs:complexType name="GegevensGevolmachtigde">
		<xs:annotation>
			<xs:documentation>Gegevens van de persoon die namens de vervreemder(s) of de verkrijger(s) optreedt.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="persoonGegevens" type="tia:GBA_Ingezetene" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Persoonlijke gegevens van de gevolmachtigde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adresKantoor" type="tia:BAG_BinnenlandsAdres" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Adres van het kantoor of van de notarissen waar de gevolmachtigde werkzaam is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adresPersoon" type="tia:IMKAD_AdreskeuzeWl" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation>Persoonlijk woonadres van de gevolmachtigde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="legitimatiebewijs" type="tia:Legitimatiebewijs" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Legitimatiegegevens van de gevolmachtigde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kadGegevensPersoon" type="tia:IMKAD_KadNatuurlijkPersoon" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Persoongegevens van de gevolmachtigde indien deze afwijken van de GBA gegevens.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="StukdeelDoorhalingHypotheek" type="tia:StukdeelDoorhalingHypotheek" substitutionGroup="tia:IMKAD_Stukdeel"/>
	<xs:complexType name="StukdeelDoorhalingHypotheek">
		<xs:annotation>
			<xs:documentation>Gegevens van een stukdeel betreffende een doorhaling van een hypotheek.
Uitganspunt voor de structuur van de XSD en de opzet van het automatisch verwerken, is dat ieder stukdeel slechts 1 rechtsfeit betreft. Dit houdt bij doorhalingen in dat in een stuk, met meerdere doorhalingen, iedere doorhaling resulteert in 1 stukdeel.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tia:IMKAD_Stukdeel">
				<xs:sequence>
					<xs:element name="doorTeHalenStuk" type="tia:DoorTeHalenStuk" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>De gegevens van het stuk betreffende de hypotheek die doorgehaald wordt.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="soortDoorhaling" type="alg:Waardenlijst" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Geeft de soort doorhaling aan: afstand hypotheekrecht, opzegging hypotheektrecht e.d. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="akteOpzeggingsBevoegdheid" type="tia:AkteOpzeggingsbevoegdheid" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Gegevens van de akte waarin de afstand al is aangenomen, en waarnaar verwezen wordt bij de aanvaarding van afstand.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="volmachtverleningAfstand" type="tia:VolmachtverleningAfstand" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Gegevens van de akte waarin de afstand al is aangenomen, en waarnaar verwezen wordt bij de aanvaarding van afstand.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="debiteurAkteHypotheekrecht" type="xs:string" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Namen van een debiteur die genoemd wordt in de akte waarbij het hypotheekrecht gevestigd werd. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="voormaligeRechtspersoonRef" type="tia:VoormaligeRechtspersoon" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Per StukdeelDoorhalingHypotheek wordt aangegeven welke voormalige rechtspersoon bij het stukdeel hoort. De bijbehorende IMKAD_Persoon moet tot het zelfde Stukdeel behoren.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="DoorTeHalenStuk" type="tia:DoorTeHalenStuk"/>
	<xs:complexType name="DoorTeHalenStuk">
		<xs:annotation>
			<xs:documentation>Gegevens van het stuk dat doorgehaald moet worden.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deelEnNummer" type="tia:IMKAD_DeelEnNummer" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identificatie van het stuk dat doorgehaald moet worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumInschrijving" type="xs:date" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het tijdstip waarop een ter inschrijving aangeboden stuk is ontvangen met in achtneming van de openingstijden en -dagen van het Kadaster. Als tijdstip van inschrijving geldt het tijdstip van aanbieding van de voor de inschrijving vereiste stukken. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Hoedanigheid" type="tia:Hoedanigheid"/>
	<xs:complexType name="Hoedanigheid">
		<xs:sequence minOccurs="0">
			<xs:element name="tekstKeuze" type="tia:TekstKeuze" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tekst die door een aanbieder gekozen is voor een variabele tekst.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wordtVertegenwoordigdRef" type="tia:IMKAD_Persoon" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID" use="optional">
			<xs:annotation>
				<xs:documentation>Technisch ID voor verwijzing naar een Hoedanigheid</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="xlink:simpleLink"/>
	</xs:complexType>
	<xs:element name="Notaris" type="tia:Notaris"/>
	<xs:complexType name="Notaris">
		<xs:annotation>
			<xs:documentation>De gegevens van de notaris die de akte heeft ondertekend (ondertekenaar) of equivalent verklaard (verklaarder). Eventueel als (vacature) waarnemer van een andere notaris.  </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="persoonsgegevens" type="tia:GBA_Ingezetene" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De persoonsgegevens van de notaris.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="voorletters" type="alg:alg-AN50" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De voorletters van de notaris die de akte passeert.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="standplaats" type="alg:alg-AN80zonderVoorloopspatie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De vestigingsplaats van het notariskantoor. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gemeente" type="alg:alg-AN80zonderVoorloopspatie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De gemeente waar het notariskantoor gevestigd is. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tekstkeuze" type="tia:TekstKeuze" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Tekst die door een aanbieder gekozen is voor een variabele tekst.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isWaarnemerVoor" type="tia:Notaris" minOccurs="0" maxOccurs="1"/>
			<xs:element name="isVacatureWaarnemerVoor" type="tia:Notaris" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Softwareleverancier" type="tia:Softwareleverancier"/>
	<xs:complexType name="Softwareleverancier">
		<xs:annotation>
			<xs:documentation>Gegevens softwareleverancier. Wordt niet voor de registratie gebruikt. Wordt gebruikt om in de aangeleverde XML te kunnen achterhalen met welke software deze is gemaakt.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bedrijfsnaam" type="alg:alg-AN80" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Naam van de softwareleverancier.

Indien dit gegeven constant gehouden wordt dan kan op dit gegeven gefilterd worden t.b.v. analyses e.d.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naam" type="alg:alg-AN80" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Naam van het softwarepakket.

Indien dit gegeven constant gehouden wordt dan kan op dit gegeven gefilterd worden t.b.v. analyses e.d.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versie" type="alg:alg-AN14" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Versienummer van de notarissoftware.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="VolmachtverleningAfstand" type="tia:VolmachtverleningAfstand"/>
	<xs:complexType name="VolmachtverleningAfstand">
		<xs:annotation>
			<xs:documentation>Gegevens van de akte waarin de afstand al is aangenomen, en waarnaar verwezen wordt bij de aanvaarding van afstand.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="aantalOnderhandseAkten" type="alg:alg-N3" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het aantal onderhandse akten dat ten grondslag ligt aan de volmacht voor het doen van afstand van het hypotheekrecht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOndertekening" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De datum van ondertekening van de akte van volmacht voor het doen van afstand van het hypotheekrecht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="naamNotaris" type="alg:alg-AN200" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De naam van de notaris door wie de akte van volmacht voor het doen van afstand van het hypotheekrecht ondertekend is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plaatsNotarisKantoor" type="alg:alg-AN80" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De woonplaats waar het notaris kantoor, van de notaris door wie de akte van volmacht voor het doen van afstand van het hypotheekrecht ondertekend is, gevestigd is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Bericht_TIA_Stuk" type="tia:Bericht_TIA_Stuk"/>
	<xs:complexType name="Bericht_TIA_Stuk">
		<xs:annotation>
			<xs:documentation>Definitie van het bericht dat gebruikt wordt in combinatie met stylesheets voor TIA-stukken.

Definition of the message that is used in combination with stylesheets for documents that are to be entered in the Registry.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nummerStylesheet" type="alg:alg-AN14V" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unieke identificatie van een stylesheet.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="implementatienummer" type="alg:alg-AN14" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optioneel attribuut om een keuze voor een specifieke implementatie te maken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="softwareleverancier" type="tia:Softwareleverancier" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gegevens softwareleverancier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="tia:IMKAD_AangebodenStuk" minOccurs="1" maxOccurs="1"/>
			<xs:element name="partnerSpecifiek" type="xs:anyType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifieke gegevens van de partner, die niet nodig zijn voor acceptatie en/of registratie van het stuk, maar alleen nodig zijn voor de opmaak van de akte.
De context van de gegevens is afhankelijk van het gebruik binnen een modeldocument en kan daardoor voor verschillende banken op verschillende wijzen gebruikt worden.
Dit element bevat de gegevens die gebruikt worden in de stylesheet en gebaseerd zijn op een XSD die door de partner is vastgesteld. De structuur hiervan is dus niet vastgelegd in deze XSD.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ds:Signature" minOccurs="0" maxOccurs="1"/>	
		</xs:sequence>	
	</xs:complexType>
	<xs:simpleType name="AanduidingGeslacht">
		<xs:annotation>
			<xs:documentation>Het geslacht van een natuurlijk persoon. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Man"/>
			<xs:enumeration value="Vrouw"/>
			<xs:enumeration value="Onbekend"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="IMKAD_Ondertekenaar" type="tia:IMKAD_Ondertekenaar"/>
	<xs:complexType name="IMKAD_Ondertekenaar">
		<xs:annotation>
			<xs:documentation>De gegevens van de persoon die de minuut heeft ondertekend.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="standplaats" type="alg:alg-AN80" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Adres of standplaats/plaats van vestiging van de ondertekenaar. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_Ondertekenaar" type="tia:GBA_Ingezetene" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De gegevens van de persoon die de minuut heeft ondertekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_WaarnemerVoor" type="tia:GBA_Ingezetene" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>De gegevens van de persoon voor wie de ondertekenaar waargenomen heeft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_Initialen" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Initialen van de persoon die de equivalentieverklaring heeft ondertekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="tia_Gemeente" type="alg:alg-AN80" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gemeente waar de notaris gevestigd is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Verklaarder" type="tia:Verklaarder"/>
	<xs:complexType name="Verklaarder">
		<xs:annotation>
			<xs:documentation>Gegevens van de equivalentieverklaring en van de persoon die de equivalentieverklaring heeft ondertekend.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="verklaarder" type="tia:GBA_Ingezetene" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gegevens van de persoon die de equivalentieverklaring heeft ondertekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="waarnemerVoor" type="tia:GBA_Ingezetene" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gegevens van de persoon voor wie door de verklaarder waargenomen heeft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="standplaats" type="alg:alg-AN80zonderVoorloopspatie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Standplaats van de persoon die de equivalentieverklaring heeft ondertekend. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verklaringBestuursorgaan" type="tia:VerklaringBestuursorgaan" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gegevens van een verklaring t.b.v. bestuursorganen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gemeente" type="alg:alg-AN80zonderVoorloopspatie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Gemeente waar de notaris gevestigd is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kadGegevensVerklaarder" type="tia:IMKAD_KadNatuurlijkPersoon" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Persoongegevens van de verklaarder indien deze afwijken van de GBA gegevens.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="IMKAD_Perceel" type="tia:IMKAD_Perceel" substitutionGroup="tia:IMKAD_OnroerendeZaak"/>
	<xs:complexType name="IMKAD_Perceel">
		<xs:annotation>
			<xs:documentation>Een perceel is Onroerende zaak, onderkend als een deel van het Nederlands grondgebied van welk deel de Dienst de begrenzing met behulp van landmeetkundige gegevens heeft vastgelegd op grond van gegevens betreffende de rechtstoestand, bestemming en het gebruik en dat door zijn kadastrale aanduiding is gekenmerkt. (Definitie is ontleend aan art 1, Kadasterwet). </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tia:IMKAD_OnroerendeZaak">
				<xs:sequence>
					<xs:element name="grootte" type="alg:alg-D8" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Een door de Dienst op grond van de kadastrale grenzen van een perceel berekende indicatieve grootte van een perceel ten tijde van vorming van dat perceel. Uitgedrukt in centiares.
De grootte van een perceel geeft de waarde van de oppervlakte aan en is dus per definitie positief. Door afrondingen is de grootte altijd groter dan of gelijk aan 0,5 ca.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="tia_aantalTekeningen" type="alg:alg-N3" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Aantal  tekeningen  waarop de grens van het perceel is afgebeeld en dat aan de akte gehecht is.
Gebruikt in akte van levering.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="tia_SplitsingsverzoekOrdernummer" type="alg:alg-AN50N" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Unieke identificatie van het splitsingsverzoek</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="stukVerificatiekosten" type="tia:IMKAD_DeelEnNummer" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="Ligging">
		<xs:annotation>
			<xs:documentation>Mogelijke waarden voor de ligging van een perceel.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="nabij"/>
			<xs:enumeration value="te"/>
			<xs:enumeration value="tegenover"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="IMKAD_Leidingnetwerk" type="tia:IMKAD_Leidingnetwerk" substitutionGroup="tia:IMKAD_OnroerendeZaak"/>
	<xs:complexType name="IMKAD_Leidingnetwerk">
		<xs:annotation>
			<xs:documentation>Een leidingnetwerk is een onroerende zaak, welke bestemd is voor transport van telecommunicatie, olie, gas, elektra en water, dat kadastraal is aangeduid en waarvan de begrenzing cartografisch is of kan worden vastgelegd (definitie is ontleend aan art 1, Kadasterbesluit).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tia:IMKAD_OnroerendeZaak">
				<xs:sequence>
					<xs:element name="tia_Typering" type="alg:alg-AN250V" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Typerende omschrijving van het netwerk.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Brandmerk" type="tia:Brandmerk"/>
	<xs:complexType name="Brandmerk">
		<xs:annotation>
			<xs:documentation>Een unieke door het Kadaster toegekende identificatie van een schip.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="getal" type="alg:alg-AN6V" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Volgnummer van inschrijving </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rubriek" type="alg:Waardenlijst" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Letter als aanduiding van het soort schip.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aanduidingKadastervestiging" type="alg:alg-AN8" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Reekscode van de Kadastervestiging waar het schip geregistreerd is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jaar" type="alg:alg-N4" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Het jaar van registratie </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
